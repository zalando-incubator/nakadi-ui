{
  "name": "nakadi-ui",
  "version": "1.0.0",
  "description": "UI for Nakadi event bus.",
  "main": "./server/server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node ./server/server.js",
    "start:debug": "node --inspect --debug-brk ./server/server.js",
    "mock": "node ./tests/mocks/testServer.js",
    "build": "webpack --config=./webpack.config.prod.js",
    "start:prod": "NODE_ENV=production node ./server/server.js",
    "chromedriver": "chromedriver --url-base=/wd/hub --port=4444",
    "test": "npm run test:client && npm run test:server",
    "test:client": "elm-test ./client/Tests/Main.elm",
    "test:server": "istanbul cover --include-all-sources tests/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zalando-nakadi/nakadi-ui.git"
  },
  "keywords": [
    "nakadi",
    "elm"
  ],
  "author": "sergii.kamenskyi@zalando.de",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zalando-nakadi/nakadi-ui/issues"
  },
  "homepage": "https://github.com/zalando-nakadi/nakadi-ui#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "client-sessions": "^0.8.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-http-proxy": "^1.0.2",
    "express-request-id": "^1.4.0",
    "express-winston": "^2.4.0",
    "node-fetch": "^2.0.0-alpha.8",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-oauth2": "^1.4.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "ace-builds": "^1.4.3",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "0.28.5",
    "dress-code": "2.1.0",
    "elm": "^0.18.0",
    "elm-hot-loader": "^0.3.4",
    "elm-test": "0.18.2",
    "elm-webpack-loader": "^3.0.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.17.0",
    "istanbul": "^1.1.0-alpha.1",
    "jasmine": "^2.7.0",
    "jasmine-reporters": "^2.2.1",
    "node-elm-compiler": "^4.3.2",
    "style-loader": "^0.18.2",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.9",
    "webdriverio": "^4.13.2",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "browserslist": "> 0.25%, not dead"
}
